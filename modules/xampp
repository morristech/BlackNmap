echo "\033[32;1m
       ..::|[ \033[31;1mNMap Xampp Brute\033[32;1m ]|::..
"
echo "\n\n"
echo -n " \033[32;1m[\033[31;1m#\033[32;1m]\033[37;1m Masukan Host : "
read host
echo -n "\033[32;1m[\033[31;1m#\033[32;1m]\033[37;1m Masukan Port : "
read port



nmap -p $port --script xmpp-brute $host

cd /blackjack/
sh droid